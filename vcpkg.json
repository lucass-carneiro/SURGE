{
  "$schema": "https://raw.githubusercontent.com/microsoft/vcpkg-tool/main/docs/vcpkg.schema.json",
  "name": "surge",
  "version": "1.0.0",
  "dependencies": [
    {
      "name": "mimalloc",
      "features": [
        "override"
      ],
      "platform": "windows"
    },
    {
      "name": "mimalloc",
      "platform": "!windows"
    },
    "gsl-lite",
    "tl-expected",
    "yaml-cpp",
    "eastl",
    "stb",
    "glfw3",
    "glm",
    "tinyobjloader",
    "opengl",
    {
      "name": "glad",
      "features": [
        "loader",
        "gl-api-latest"
      ]
    },
    {
      "name": "imgui",
      "features": [
        "freetype",
        "glfw-binding",
        "opengl3-binding"
      ]
    },
    {
      "name": "freetype",
      "features": [
        "error-strings"
      ]
    },
    {
      "name": "tracy",
      "features": [
        "cli-tools",
        "crash-handler",
        "gui-tools",
        "on-demand"
      ]
    }
  ]
}